{
  "version": 2,
  "builds": [
    {
      "src": "start.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/dist-animation/(.*)",
      "dest": "/public/dist-animation/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|json|png|jpg|jpeg|gif|svg|ico|html|woff|woff2|ttf|eot|map))",
      "dest": "/public/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/start.js"
    }
  ]
}
